{
    "arduino.path": "~/.arduino15",
    "arduino.commandPath": "arduino-cli",
    "arduino.defaultBaudRate": 115200,
    "C_Cpp.intelliSenseEngine": "default",
    "files.associations": {
        "*.ino": "cpp",
        "array": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "ranges": "cpp",
        "span": "cpp",
        "vector": "cpp",
        "*.remove": "cpp",
        "algorithm": "cpp",
        "any": "cpp",
        "chrono": "cpp",
        "deque": "cpp",
        "filesystem": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "list": "cpp",
        "map": "cpp",
        "memory": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
        "optional": "cpp",
        "random": "cpp",
        "ratio": "cpp",
        "set": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "utility": "cpp",
        "ostream": "cpp",
        "hash_map": "cpp",
        "format": "cpp",
        "initializer_list": "cpp",
        "text_encoding": "cpp",
        "valarray": "cpp",
        "fstream": "cpp"
    },
    "arduino.board": "esp32:esp32:esp32s3",
    "arduino.defaultCore": "esp32:esp32",
    "arduino.logLevel": "info",
    "arduino.allowPDEFiletype": false,
    "arduino.enableUSBDetection": true,
    "arduino.disableIntelliSenseAutoGen": false,
    "arduino.skipHeaderProvider": false,
    "C_Cpp.errorSquiggles": "enabled",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.o": true
    }
}
